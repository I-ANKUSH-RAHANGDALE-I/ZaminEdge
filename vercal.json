"version":2,
"builds": [
{
    "src":"Zamin_edge/wsgi.py",
    "use":"@vercel/python",
    "config": {"maxLambdaSize":"15mb","runtime":"python3.9"}
}
{
    "src":"build_file.sh",
    "use":"@vercel/static-build",
    "config": {
        "distDir":"staticfile_build"
    }
}
]
"route":[
    {
        "src":"/static/(.*)",
        "dest":"/static/$1"
    },
    {
        "src":"/(.*)",
        "dest":"Zamin_edge/wsgi.py"  
    }
]